# Violin Auto-Playing Configuration
# Copy this file to .env and adjust values

# Camera settings
CAMERA_DEVICE_ID=0

# MIDI settings
MIDI_PORT_NAME=ViolinCV
MIDI_CHANNEL=0
MIDI_VELOCITY=100

# Database
DATABASE_PATH=data/performance_logs/sessions.db
DATABASE_ENABLED=true

# Debug mode
DEBUG_MODE=false
DEBUG_SHOW_LANDMARKS=true
